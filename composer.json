{
  "name": "meethour/php-sdk",
  "description": "PHP SDK of Meet Hour application - https://meethour.io",
  "type": "library",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/support": ">=5.6"
  },
  "repository": "https://github.com/v-empower/MeetHour-PHP-SDK",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Meethour\\PhpSdk\\": "src/"
    },
    "files": [
        "src\/autoload.php"
      ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MeetHourApp\\Services\\MHApiServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Meet Hour",
      "email": "hello@meethour.io"
    }
  ],
  "minimum-stability": "dev",
  "require-dev": {
    "orchestra/testbench": ">=3.8"
  }
}
